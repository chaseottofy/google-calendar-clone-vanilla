{
  "name": "google-calendar-clone-vanilla",
  "version": "1.0.1",
  "description": "zero dependency clone of google calendar written in vanilla javascript",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "analyze2": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build": "webpack --mode production",
    "dev": "webpack serve --open --mode development",
    "predeploy": "eslint . && webpack --mode production",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "start:server": "concurrently --kill-others \"json-server --watch db.json --port 3001\" \"webpack serve --open --mode development\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaseottofy/google-calendar-clone-vanilla.git"
  },
  "keywords": [
    "google-calendar",
    "html",
    "css",
    "javascript",
    "calendar",
    "clone",
    "google",
    "vanilla",
    "vanillajs"
  ],
  "author": "chase ottofy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chaseottofy/google-calendar-clone-vanilla/issues"
  },
  "homepage": "https://github.com/chaseottofy/google-calendar-clone-vanilla#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "compression-webpack-plugin": "^10.0.0",
    "concurrently": "^9.0.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "cssnano-preset-advanced": "^7.0.6",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fetch-mock": "^11.1.3",
    "gh-pages": "^5.0.0",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "json-server": "^1.0.0-beta.2",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.45",
    "postcss-csso": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.3",
    "source-map-loader": "^4.0.1",
    "stylelint": "^16.9.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-high-performance-animation": "^1.10.0",
    "stylelint-order": "^6.0.4",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-nano": "^1.1.1"
  }
}
